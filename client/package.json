{
  "name": "twc-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "react": "16.7.0-alpha.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "16.7.0-alpha.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true PORT=2891 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "cross-env SKIP_PREFLIGHT_CHECK=true pretty-quick && eslint --fix src/**/*.js",
    "precommit": "lint-staged",
    "lint-staged": {
      "*.js,jsx": [
        "pretty-quick --staged",
        "eslint src/ --fix",
        "git add"
      ]
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "cross-env": "^5.2.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.4",
    "istanbul": "^1.1.0-alpha.1",
    "lint-staged": "^8.0.5",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint --silent && cross-env CI=true npm run test --silent"
    }
  }
}
